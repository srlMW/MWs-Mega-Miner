        begin
          case random(30) of
            0..10: sleepAndMoveMouse(100 + random(50));
            11..20: sleepAndMoveMouse(300 + random(70));
            21..27: sleepAndMoveMouse(600 + random(90));
            28..30: sleepAndMoveMouse(1000 + random(110));
          end;
        end;
      8..12: pickUpMouse();
    end;
    wait(500 + random(100));
    if lootScreen.isOpen then
      lootScreen.close;
    tabBackpack.waitForShift(randomRange(players[currentPlayer].integers[2], players[currentPlayer].integers[3]));
    antiban();
  end;
end;

procedure TMSObject.Find();
var
  x, y, i: integer;
  TPA, pathToMine2: TPointArray;
  ATPA: T2DPointArray;
  miningTimer: TTimeMarker;
begin
  if tabBackPack.isfull then
    dropOres();
  if lootScreen.isOpen then
    lootScreen.close;
  miningTimer.start;
  print('Searching for rock to mine', TDebug.SUB);
  if Self.Colour.gatherIn(TPA, mainScreen.getBounds()) then
  begin
    ATPA := SplitTPAEx(TPA, 5, 10);
    ATPA.sortFromMidPoint(mainscreen.playerPoint);
    for i := 0 to high(ATPA) do
    begin
      case random(100) of
        0..9: missMouse(ATPA[i].getBounds().getGaussPoint(), true);
        10..100: mouse(ATPA[i].getBounds().getGaussPoint(), MOUSE_MOVE, MOUSE_HUMAN);
      end;
      if isMouseOverText(Self.OverText) then
        clickOre();
    end;
  end;
  if lootScreen.isOpen then
    lootScreen.close;
  stats_commit();
  failsafeTimer.start;
end;

procedure mainloop();
begin
  failsafeTimer.start;
  minimap.toggleRun(true);
  while (tabBackpack.count() < 28) or (failsafeTimer.getTime < 300000) do
    case oreType of
      'Varrock Tin': vTin.Find();
      'Varrock Copper': vCopper.Find();
      'Varrock Iron': vIron.Find();
      'Lumbridge Tin': lTin.Find();
      'Lumbridge Copper': lCopper.Find();
      'Barbarian Village Tin': bvTin.Find();
      'Falador Tin': fTin.Find();
      'Falador Copper': fCopper.Find();
      'Falador Iron': fIron.Find();
      'Rimmington Tin': rTin.Find();
      'Rimmington Copper': rCopper.Find();
      'Rimmington Iron': rIron.Find();
    end;
  failsafeTimer.start;
  dropOres();
  case statType of
    'Varrock Tin', 'Lumbridge Tin', 'Barbarian Village Tin', 'Falador Tin', 'Rimmington Tin':
      begin
        stats_IncVariable('6', 28);
        stats_IncVariable('68', 490);
      end
    'Varrock Copper', 'Lumbridge Copper', 'Falador Copper', 'Rimmington Copper':
      begin
        stats_IncVariable('5', 28);
        stats_IncVariable('68', 490);
      end
    'Varrock Iron', 'Falador Iron', 'Rimmington Iron':
      begin
        stats_IncVariable('7', 28);
        stats_IncVariable('68', 980);
      end
  end;
  stats_commit();
end;

begin
  addOnTerminate('freeTheDTMs');
  clearDebug();
  smartEnableDrawing := true;
  initPlayerForm();
  runPlayerForm();
  if (not playerForm.isScriptReady) then
    exit;
  declarePlayers();
  autoUpdateMe();
  setupSRL();
  loadObjects();
  paintDisplay();
  initDTMs();
  stats_Setup('37');
  stats_commit();
  loggingIn();
  findNormalRandoms();
  setup();
  scriptTimer.start();
  antibanTimer.start();
  repeat
    playerCheck();
    mainloop();
  until (scriptTimer.getTime() > players[currentPlayer].integers[0]);
  stats_commit();
  players[currentPlayer].logout();
  freeTheDTMs();
end.
